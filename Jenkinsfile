pipeline {
  agent any
  stages {
    stage('rrrrr') {
      steps {
        parallel(
          "rvgf": {
            sleep 2
            
          },
          "e4dc": {
            writeFile(file: 'gggg', text: 'rfdd')
            
          }
        )
      }
    }
    stage('error') {
      steps {
        parallel(
          "1": {
            echo '1'
            
          },
          "2": {
            echo '2'
            
          },
          "3": {
            sleep 3
            
          },
          "4": {
            echo '4'
            
          }
        )
      }
    }
  }
}